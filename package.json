{
  "name": "@singularitsas/octopus-cli",
  "version": "1.0.0",
  "main": "bin/index.js",
  "description": "A CLI to generate krakend endpoints",
  "author": "Ernesto Luis Mosquera Heredia <ernesto.mosquera@singularit.co>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "cli",
    "krakend",
    "openapi",
    "api-gateway",
    "microservices",
    "singularit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/singularit/octopus-cli.git"
  },
  "homepage": "https://github.com/singularit/octopus-cli#readme",
  "bugs": {
    "url": "https://github.com/singularit/octopus-cli/issues"
  },
  "scripts": {
    "prebuild": "rimraf bin",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"{src,apps,libs,__test__}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"__test__/**/*.ts\""
  },
  "dependencies": {
    "gradient-string": "^2.0.1",
    "js-yaml": "^4.1.0",
    "node-fetch": "2.6.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "bin": {
    "octo-singular": "bin/index.js"
  }
}
